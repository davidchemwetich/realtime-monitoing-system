{
    "id": null,
    "uid": "laravel-monitoring",
    "title": "Laravel Real-time Monitoring Dashboard",
    "tags": [
        "laravel",
        "monitoring",
        "real-time"
    ],
    "timezone": "browser",
    "refresh": "30s",
    "schemaVersion": 39,
    "version": 1,
    "time": {
        "from": "now-1h",
        "to": "now"
    },
    "timepicker": {
        "refresh_intervals": [
            "5s",
            "10s",
            "30s",
            "1m",
            "5m",
            "15m",
            "30m",
            "1h",
            "2h",
            "1d"
        ]
    },
    "panels": [
        {
            "id": 1,
            "title": "Requests Per Minute",
            "type": "stat",
            "gridPos": {
                "h": 8,
                "w": 6,
                "x": 0,
                "y": 0
            },
            "targets": [
                {
                    "expr": "rate(laravel_pulse_requests_per_minute[5m]) * 60",
                    "legendFormat": "Requests/min",
                    "refId": "A"
                }
            ],
            "fieldConfig": {
                "defaults": {
                    "unit": "reqps",
                    "color": {
                        "mode": "thresholds"
                    },
                    "thresholds": {
                        "steps": [
                            {
                                "color": "green",
                                "value": null
                            },
                            {
                                "color": "yellow",
                                "value": 50
                            },
                            {
                                "color": "red",
                                "value": 100
                            }
                        ]
                    }
                }
            }
        },
        {
            "id": 2,
            "title": "Response Times (Slow Requests)",
            "type": "timeseries",
            "gridPos": {
                "h": 8,
                "w": 6,
                "x": 6,
                "y": 0
            },
            "targets": [
                {
                    "expr": "laravel_pulse_slow_requests_total",
                    "legendFormat": "Slow Requests (>1000ms)",
                    "refId": "A"
                }
            ],
            "fieldConfig": {
                "defaults": {
                    "unit": "short",
                    "color": {
                        "mode": "palette-classic"
                    }
                }
            }
        },
        {
            "id": 3,
            "title": "Queue Lengths",
            "type": "timeseries",
            "gridPos": {
                "h": 8,
                "w": 6,
                "x": 12,
                "y": 0
            },
            "targets": [
                {
                    "expr": "laravel_queue_size",
                    "legendFormat": "Queue: {{queue}}",
                    "refId": "A"
                }
            ],
            "fieldConfig": {
                "defaults": {
                    "unit": "short",
                    "color": {
                        "mode": "thresholds"
                    },
                    "thresholds": {
                        "steps": [
                            {
                                "color": "green",
                                "value": null
                            },
                            {
                                "color": "yellow",
                                "value": 50
                            },
                            {
                                "color": "red",
                                "value": 100
                            }
                        ]
                    }
                }
            }
        },
        {
            "id": 4,
            "title": "Failed Jobs",
            "type": "stat",
            "gridPos": {
                "h": 8,
                "w": 6,
                "x": 18,
                "y": 0
            },
            "targets": [
                {
                    "expr": "laravel_queue_failed_jobs_total",
                    "legendFormat": "Failed Jobs",
                    "refId": "A"
                }
            ],
            "fieldConfig": {
                "defaults": {
                    "unit": "short",
                    "color": {
                        "mode": "thresholds"
                    },
                    "thresholds": {
                        "steps": [
                            {
                                "color": "green",
                                "value": null
                            },
                            {
                                "color": "yellow",
                                "value": 5
                            },
                            {
                                "color": "red",
                                "value": 10
                            }
                        ]
                    }
                }
            }
        },
        {
            "id": 5,
            "title": "Broadcasts Per Minute (Horizon Jobs)",
            "type": "timeseries",
            "gridPos": {
                "h": 8,
                "w": 12,
                "x": 0,
                "y": 8
            },
            "targets": [
                {
                    "expr": "laravel_horizon_jobs_per_minute",
                    "legendFormat": "Jobs/min",
                    "refId": "A"
                }
            ],
            "fieldConfig": {
                "defaults": {
                    "unit": "short"
                }
            }
        },
        {
            "id": 6,
            "title": "Job Processing Rates",
            "type": "timeseries",
            "gridPos": {
                "h": 8,
                "w": 12,
                "x": 12,
                "y": 8
            },
            "targets": [
                {
                    "expr": "laravel_horizon_recent_jobs_total",
                    "legendFormat": "Recent Jobs Processed",
                    "refId": "A"
                }
            ],
            "fieldConfig": {
                "defaults": {
                    "unit": "short"
                }
            }
        },
        {
            "id": 7,
            "title": "Memory Usage",
            "type": "timeseries",
            "gridPos": {
                "h": 8,
                "w": 8,
                "x": 0,
                "y": 16
            },
            "targets": [
                {
                    "expr": "laravel_app_memory_usage_bytes",
                    "legendFormat": "Current Memory",
                    "refId": "A"
                },
                {
                    "expr": "laravel_app_memory_peak_bytes",
                    "legendFormat": "Peak Memory",
                    "refId": "B"
                }
            ],
            "fieldConfig": {
                "defaults": {
                    "unit": "bytes"
                }
            }
        },
        {
            "id": 8,
            "title": "Cache Effectiveness",
            "type": "timeseries",
            "gridPos": {
                "h": 8,
                "w": 8,
                "x": 8,
                "y": 16
            },
            "targets": [
                {
                    "expr": "laravel_pulse_cache_effectiveness_percent",
                    "legendFormat": "Cache Hit Rate %",
                    "refId": "A"
                }
            ],
            "fieldConfig": {
                "defaults": {
                    "unit": "percent",
                    "min": 0,
                    "max": 100
                }
            }
        },
        {
            "id": 9,
            "title": "Database Connections",
            "type": "timeseries",
            "gridPos": {
                "h": 8,
                "w": 8,
                "x": 16,
                "y": 16
            },
            "targets": [
                {
                    "expr": "laravel_database_connections_active",
                    "legendFormat": "Active Connections",
                    "refId": "A"
                }
            ],
            "fieldConfig": {
                "defaults": {
                    "unit": "short"
                }
            }
        },
        {
            "id": 10,
            "title": "Exception Rate",
            "type": "timeseries",
            "gridPos": {
                "h": 8,
                "w": 12,
                "x": 0,
                "y": 24
            },
            "targets": [
                {
                    "expr": "laravel_pulse_exceptions_total",
                    "legendFormat": "Exceptions (5min)",
                    "refId": "A"
                }
            ],
            "fieldConfig": {
                "defaults": {
                    "unit": "short",
                    "color": {
                        "mode": "thresholds"
                    },
                    "thresholds": {
                        "steps": [
                            {
                                "color": "green",
                                "value": null
                            },
                            {
                                "color": "yellow",
                                "value": 3
                            },
                            {
                                "color": "red",
                                "value": 5
                            }
                        ]
                    }
                }
            }
        },
        {
            "id": 11,
            "title": "Cache Operations",
            "type": "timeseries",
            "gridPos": {
                "h": 8,
                "w": 12,
                "x": 12,
                "y": 24
            },
            "targets": [
                {
                    "expr": "laravel_pulse_cache_hits_total",
                    "legendFormat": "Cache Hits",
                    "refId": "A"
                },
                {
                    "expr": "laravel_pulse_cache_misses_total",
                    "legendFormat": "Cache Misses",
                    "refId": "B"
                }
            ],
            "fieldConfig": {
                "defaults": {
                    "unit": "short"
                }
            }
        }
    ]
}